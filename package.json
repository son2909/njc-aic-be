{
  "name": "nest-base-serverless",
  "version": "0.0.1",
  "description": "nest-base-serverless",
  "author": "HuuSon",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"*.js\" \"*.json\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:ws": "serverless offline start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix ./src --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "lambda:start": "nest build && sls offline start --reloadHandler",
    "lambda:build": "nest build && sls package",
    "lambda:deploy": "nest build && sls deploy",
    "socket:dev": "nest build --webpack --watch",
    "prepare": "husky install"
  },
  "pre-commit": [],
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^7.1.0",
    "@sparticuz/chromium": "^116.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/node": "^18.7.5",
    "@vendia/serverless-express": "^4.10.3",
    "archiver": "^6.0.1",
    "aws-lambda": "^1.0.7",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dayjs": "^1.11.9",
    "ejs": "^3.1.9",
    "fs-extra": "^11.1.1",
    "iconv-lite": "^0.6.3",
    "linebyline": "^1.3.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mysql2": "^3.3.3",
    "passport": "^0.6.0",
    "puppeteer-core": "^21.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7",
    "typeorm": "^0.2.25",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@types/aws-lambda": "^8.10.115",
    "@types/express": "^4.17.3",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "aws-sdk": "^2.1278.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "26.0.1",
    "lint-staged": "^13.2.2",
    "plugin": "^0.3.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "run-script-webpack-plugin": "^0.2.0",
    "serverless": "^3.33.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-jetpack": "^0.11.2",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "serverless-prune-plugin": "^2.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.0",
    "webpack-cli": "^5.1.1",
    "webpack-node-externals": "^3.0.0"
  },
  "_moduleAliases": {
    "@config": "dist/src/config/",
    "@validators": "dist/src/validators/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
